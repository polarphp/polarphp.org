                    <li><a href="/about/becomepartner.html"><%= __("about.menu_become_partner") %></a></li>
                </ul>
            </div>
            <div className="about-sponsors-container">
                coming soon...
            </div>
        </div>
    </div>
</div>
<script>
   $(".about-container .about-nav a").click(function(){
      window.location.assign($(this).attr("href"));
   });
</script>